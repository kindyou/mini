<!DOCTYPE html>
<html>
<%- partial('_partial/head') %>


<body>

    
    <div class="container">

    <%#- partial('_partial/header') %>
    <%- body %>
    
    <%# 这是目录的内容 %>
    <div class="post-directory" style="margin-top:100px;">
    <h4>All Posts</h4>
      <% site.posts.forEach(function(post) { %>
        <a href="<%= config.root %><%= post.path %>"><%= post.title %></a></br>
      <% }); %>
    </div>



    <%# 这是全局的footer内容 %>
    <%- partial('_partial/footer') %>
    </div>

</body>
</html> 



<style>

@media screen and (min-width: 565px) { 
  /* 当屏幕宽度大于等于500px时，应用这些样式 */ 
  body { margin:120px 120px 0px 120px;width:525px;font-size:15px;font-family:Georgia, serif; } 
}

@media screen and (max-width: 564px) { 
  /* 当屏幕宽度小于500px时，应用这些样式 */ 
  body { margin:120px 30px 0px 30px;font-size:15px;font-family:Georgia, serif; } 
}
  
  </style>
